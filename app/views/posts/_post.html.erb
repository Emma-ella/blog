<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>Author name:</strong>
    <%= post.author_name %>
  </p>

  <p>
    <strong>Category name:</strong>
    <%= post.category %>
  </p>


  <p>
    <strong>Published at:</strong>
    <%= post.published_at %>
  </p>

</div>
